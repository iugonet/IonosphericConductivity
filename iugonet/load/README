# for initialize DATABASE
sqlite3 igrf11.db < igrf11.sql
sqlite3 iri2012.db < iri2012.sql
sqlite3 nrlmsise00.db < nrlmsise00.sql
sqlite3 ionospheric_cond.db < ionospheric_cond.sql

# To check inside DATABASE (in case of igrf11) 
$ sqlite3 igrf11.db
sqlite> .schema
sqlite> select * from igrf11;
sqlite> .quit

# To delete all records (in case of igrf11)
$ sqlite3 igrf11.db
sqlite> delete from igrf11;
sqlite> .quit

# To remove DATABASE (in case of igrf11)
$ rm igrf11.db
